Algoritmo sin_titulo
	definir cade, cade2, cade3, cade4, cade5 Como Caracter;
	leer cade;
	cade2 <- subcadena(cade,1,1);
	cade3 <- subcadena(cade,2,2);
	cade4 <- subcadena(cade,3,3);
	cade5 <- subcadena(cade,4,4);
	Escribir cade2;
	Escribir cade3;
	Escribir cade4;
	Escribir cade5;
	
	Si Longitud(cade)>2 Entonces
		Escribir "Sigale mijo";
	SiNo
		Escribir "No sabes leer o que?  >:(";
	Fin Si
	
	
FinAlgoritmo
